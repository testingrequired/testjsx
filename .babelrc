{
  "sourceMaps": "inline",
  "presets": ["@babel/preset-env"],
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      {
        "pragma": "TestJsx.h", // default pragma is React.createElement
        "pragmaFrag": "TestJsx.Fragment", // default is React.Fragment
        "throwIfNamespace": false // defaults to true
      }
    ]
  ]
}
